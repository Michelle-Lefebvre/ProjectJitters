{% extends "/admin/users_master.html.twig" %}

{% block title %}User List{% endblock %}

{% block content %}
<h1>User List</h1>

<table class="center">
<tr>
    <th>User Id</th>
    <th>Email</th>
    <th>Phone</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Nickname</th>
    <th>Rewards</th>
    <th>Photo</th>
    <th>Created On</th>
    <th>Admin User</th>
    <th>Actions</th>
</tr>
    {% for u in usersList %}
<tr>
    <td>{{u.userId}}</td>
    <td>{{u.email}}</td>
    <td>{{u.mobilePhone}}</td>
    <td>{{u.firstName}}</td>
    <td>{{u.lastName}}</td>
    <td>{{u.nickname}}</td>
    <td>{{u.rewards}}</td>
    <td>{{u.photoFilePath}}</td>
    <td>{{u.creationTS}}</td>
    <td>{{u.adminuser}}</td>
    <td><a href="/admin/users/edit{{u.userId}}">edit</a></td>
    <td><a href="/admin/users/delete{{u.userId}}">delete</a></td>
</tr>

    {% endfor %}
</table>  
{% endblock %}