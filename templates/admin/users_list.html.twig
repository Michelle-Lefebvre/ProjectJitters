{% extends "/admin/master.html.twig" %}

{% block title %}User List{% endblock %}

{% block content %}
<h2>User List</h2>

<table border="1" class="center">
<tr>
    <th>User Id</th>
    <th>Email</th>
    <th>Phone</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Nickname</th>
    <th>Rewards</th>
    <th>Photo</th>
    <th>Created On</th>
    <th>Admin User</th>
    <th colspan="2">Actions</th>
</tr>
    {% for u in usersList %}
<tr>
    <td>{{u.userId}}</td>
    <td>{{u.email}}</td>
    <td>{{u.mobilePhone}}</td>
    <td>{{u.firstName}}</td>
    <td>{{u.lastName}}</td>
    <td>{{u.nickname}}</td>
    <td>{{u.rewards}}</td>
    <td>{{u.photoFilePath}}</td>
    <td>{{u.creationTS}}</td>
    <td>{{u.adminuser}}</td>
    <td><a href="/admin/users/{{u.userId}}/edit">edit</a></td>
    <td><a href="/admin/users/{{u.userId}}/delete">delete</a></td>
</tr>

    {% endfor %}
</table>  
{% endblock %}